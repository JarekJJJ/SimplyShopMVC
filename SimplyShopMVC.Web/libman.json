{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery.cookie@1.4.1",
      "destination": "wwwroot/lib/jquery.cookie/",
      "files": [
        "jquery.cookie.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@4.4.1",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "chart.min.js",
        "chart.js"
      ]
    }
  ]
}